{
  "name": "nko2017-gm5",
  "version": "1.0.0",
  "description": "- A game to play bomberman",
  "main": "src/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "reload": "livereload 'build/'",
    "clean": "rm -rf build/js/",
    "build:dev": "npm run clean && rollup -c rollup.config.js",
    "build-run:dev": "npm run clean && rollup -c rollup.config.js && node ./src/utils/server.js",
    "build:prod": "npm run clean && NODE_ENV=production rollup -c rollup.config.js",
    "start": "npm run build-run:dev",
    "server": "node ./src/utils/server.js",
    "postinstall": "npm run build:prod"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Hackbit/nko2017-gm5.git"
  },
  "keywords": [
    "nko2017",
    "node",
    "socketio"
  ],
  "author": "tingwei628",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Hackbit/nko2017-gm5/issues"
  },
  "homepage": "https://github.com/Hackbit/nko2017-gm5#readme",
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-preset-env": "^1.6.1",
    "cssnano": "^3.10.0",
    "rollup": "^0.51.3",
    "rollup-plugin-babel": "^3.0.2",
    "rollup-plugin-commonjs": "^8.2.6",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-postcss": "^0.5.5",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-uglify": "^2.0.1"
  },
  "dependencies": {
    "express": "^4.16.2"
  },
  "engines": {
    "node": "6.x.x"
  }
}